{
  "manifest_version": 2,
  "name": "FileSaver",
  "version": "1.0",
  "description": "Save files with ease",
  "permissions": ["downloads",  "storage"],
 
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon16.png"
    },
    "default_popup": "popup.html"
  }
}
